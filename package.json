{
  "name": "bablechat",
  "version": "1.0.0",
  "description": "Simple nodejs chat server following airbnb guidelines and compiled using babeljs.io",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require babel-register",
    "start": "nodemon lib/index.js --exec babel-node",
    "build": "babel lib -d dist/BabelChat",
    "serve": "node dist/BabelChat/index.js",
    "lint": "eslint lib"
  },
  "author": "Kaitlyn Callow",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1"
  },
  "dependencies": {
    "airbnb-browser-shims": "^2.0.0",
    "md5": "^2.2.1",
    "mysql": "^2.15.0",
    "socket.io": "^2.0.3"
  }
}
